<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<context:annotation-config></context:annotation-config>
	
   <bean class="com.demo.Student" id="pre">
    <property name="stdid" value="101"></property>
   <property name="stdname" value="preethi"></property>
   <property name="stdmarks" value="300"></property>
   <property name="stdadd" value="chennai"></property>
   <property name="subjects">
   <list>
   <value>Telugu</value>
   <value>Hindi</value>
   <value>English</value>
   <value>maths</value>
   <value>physics</value>
   </list>
   
   </property>
   </bean>
 	
	
	
</beans>